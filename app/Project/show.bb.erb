<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @project.project_code %></h1>
    <%= link_to "Edit", :action => :edit, :id => @project.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">ProjectCode</td>
        <td class="itemValue"><%= @project.project_code%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">ProjectName</td>
        <td class="itemValue"><%= @project.project_name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">ProjectDescription</td>
        <td class="itemValue"><%= @project.project_description%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">ProjectDuration</td>
        <td class="itemValue"><%= @project.project_duration%></td>
      </tr>
      
    </table>
  </div>

</div>
