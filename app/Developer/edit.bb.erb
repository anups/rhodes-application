<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @developer.object %>
    <h1>Edit <%= @developer.developer_code%></h1>
    <%= link_to "Delete", :action => :delete, :id => @developer.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @developer.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Developer code: </td>
              <td class="itemValue"><input type="text" name="developer[developer_code]" value="<%= @developer.developer_code %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Developer name: </td>
              <td class="itemValue"><input type="text" name="developer[developer_name]" value="<%= @developer.developer_name %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Developer status: </td>
              <td class="itemValue"><input type="text" name="developer[developer_status]" value="<%= @developer.developer_status %>"/></td>
          </tr>
          
          <tr>
              <td class="itemLabel">Developer dob: </td>
              <td class="itemValue"><input type="text" name="developer[developer_dob]" value="<%= @developer.developer_dob %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
