<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1>New Developer</h1>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @developer.object %>"/>
      <table>
          
            <tr>
                <td class="itemLabel">Developer code: </td>
                <td class="itemValue"><input type="text" name="developer[developer_code]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Developer name: </td>
                <td class="itemValue"><input type="text" name="developer[developer_name]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Developer status: </td>
                <td class="itemValue"><input type="text" name="developer[developer_status]"/></td>
            </tr>
          
            <tr>
                <td class="itemLabel">Developer dob: </td>
                <td class="itemValue"><input type="text" name="developer[developer_dob]"/></td>
            </tr>
          
      </table>
      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
