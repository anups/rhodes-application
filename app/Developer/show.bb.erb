<div data-role="page" data-add-back-btn="true">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :index %>
    <h1><%= @developer.developer_code %></h1>
    <%= link_to "Edit", :action => :edit, :id => @developer.object %>
  </div>

  <div data-role="content">
    <table>
      
      <tr>
    	<td class="itemLabel">DeveloperCode</td>
        <td class="itemValue"><%= @developer.developer_code%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">DeveloperName</td>
        <td class="itemValue"><%= @developer.developer_name%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">DeveloperStatus</td>
        <td class="itemValue"><%= @developer.developer_status%></td>
      </tr>
      
      <tr>
    	<td class="itemLabel">DeveloperDob</td>
        <td class="itemValue"><%= @developer.developer_dob%></td>
      </tr>
      
    </table>
  </div>

</div>
